are(e) {\n  state.clicked += 1;\n  const { id } = e.target;\n  document.getElementById(id).outerHTML = '';\n  createSquares(state.divideBy);\n}\n\ncreateSquares(state.start);\n"},"lineCount":null}},"error":null,"hash":"a7973e13e67514eadd09fd67e5194877","cacheData":{"env":{}}}